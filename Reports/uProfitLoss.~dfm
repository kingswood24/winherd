  object qEI: TQuery
    AfterOpen = qEIAfterOpen
    DatabaseName = 'Accs'
    SQL.Strings = (
      'SELECT DISTINCT G.GroupDesc, Sum(N.Balance) as Balance'
      'FROM nlaccs n'
      'LEFT JOIN Groups G ON (G.GroupNo= N.RepGroup)'
      'WHERE N.Category = "EI" and N.NominalType <> "P"'
      'AND N.Name <> '#39#39
      'AND N.Balance <> 0'
      'GROUP BY G.GroupDesc')
    Top = 2
    object qEIBalance: TFloatField
      FieldName = 'Balance'
      Origin = 'ACCS."nlaccs.DB".Balance'
      currency = True
    end
    object qEIGroupDesc: TStringField
      FieldName = 'GroupDesc'
      Origin = 'ACCS."groups.DB".GroupDesc'
      Size = 30
    end
  end
